  "license": "MIT",
  "devDependencies": {
    "@pact-foundation/pact": "^10.3.1",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "^9.1.1",
    "axios": "^1.2.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "express": "^4.18.1",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
